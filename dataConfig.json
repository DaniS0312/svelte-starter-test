{
    "dataSource": "data.json",
    "limit": 4,
    "offset": 0,
    "enableMultiSelect": true,
    "enableGlobalSearch": true,
    "clickableColumn": "name",
    "enableRefresh": true,
    "columns": {
        "name": {
            "sortBy": true,
            "isSortAscending": true,
            "filterBy": {
                "filterText": { "userInput": "", "placeholder": "First Name", "defaultValue": "", "type": "text" }
            }
        },
        "category": {
            "sortBy": false,
            "isSortAscending": true,
            "filterBy": {
                "filterDropdown": {
                    "userInput": [],
                    "placeholder": "-Please Select-",
                    "defaultOptions": ["Miller", "Tinkerer", "Hotdog", "Oven"]
                }
            }
        },

        "age": {
            "sortBy": false,
            "isSortAscending": true,
            "filterBy": {
                "filterNumber": { "userInput": "", "placeholder": "Age", "defaultValue": "" }
            }
        },
        "date": {
            "sortBy": false,
            "isSortAscending": true,
            "filterBy": {
                "fromDate": {
                    "userInput": "",
                    "label": "From",
                    "placeholder": "From",
                    "defaultValue": ""
                },
                "toDate": { "userInput": "", "label": "To", "placeholder": "To", "defaultValue": "" }
            }
        }
    },
    "customActions": {
        "columnHeading": "Actions",
        "actions": [
            {
                "faIcon": "faEye",
                "btnClasses": "btn-info",
                "displayHtml": null,
                "clickEvent": "open_clicked"
            },
            {
                "faIcon": "faEdit",
                "btnClasses": "btn-success",
                "displayLabel": "Edit",
                "displayHtml": null,
                "clickEvent": "edit_clicked"
            },
            {
                "faIcon": "faTrash",
                "btnClasses": "btn-error",
                "displayHtml": null,
                "clickEvent": "delete_clicked"
            }
        ]
    },
    "multiSelectActions": [
        {
            "iconUrl": "none",
            "displayLabel": "Delete Selected",
            "displayHtml": null,
            "clickEvent": "delete_clicked"
        }
    ]
}
